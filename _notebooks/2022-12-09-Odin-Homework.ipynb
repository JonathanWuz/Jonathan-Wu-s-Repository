    "# Working Code\n",
    "import random\n",
    "\n",
    "def roll(n):\n",
    "    rollsList = []\n",
    "    \n",
    "    while n > 0:\n",
    "        rollsList.append(random.randint(1,20))\n",
    "        n -= 1\n",
    "\n",
    "    print(rollsList)\n",
    "    return max(rollsList)\n",
    "\n",
    "roll(3)"
   ]
  }
